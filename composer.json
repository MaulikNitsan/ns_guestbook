{
  "name": "nitsan/ns-guestbook",
  "type": "typo3-cms-extension",
  "description": "Easy to install and configured to provide rich features of Guestbook form, send mail, review and moderate guest entries etc.. Know more from extension manual.",
  "homepage": "https://extensions.typo3.org/extension/ns_guestbook/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "Guestbook",
    "Nitsan",
    "Guest",
    "Book",
    "Feedback",
    "Review"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_guestbook/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsGuestbook\\": "Classes/"
    }
  },
  "replace": {
      "ns_guestbook": "self.version",
      "nitsan/ns-guestbook": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "extension-key": "ns_guestbook"
      }
  }
}
